{
  "overrides": [
    {
      "files": ["*.jsx", "*.js"]
    }
  ],
  "env": {
    "browser": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "import/prefer-default-export": "off"
  }
}
